            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="open" mnemonicParsing="false" onAction="#openOnAction" text="Open" />
                    <MenuItem fx:id="save" mnemonicParsing="false" onAction="#saveOnAction" text="Save" />
                    <MenuItem fx:id="delete" mnemonicParsing="false" onAction="#deleteOnAction" text="Delete" />
                    <MenuItem fx:id="newFile" mnemonicParsing="false" onAction="#newOnAction" text="New File" />
                  </items>
                </Menu>
                <Menu fx:id="info" mnemonicParsing="false" text="Info">
                  <items>
                    <MenuItem fx:id="about" mnemonicParsing="false" onAction="#aboutOnAction" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <TextArea fx:id="txtArea" BorderPane.alignment="CENTER" />
