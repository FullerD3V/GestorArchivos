<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestorarchivos.CreateFile.CreateFileController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Pane fx:id="Title" prefHeight="40.0" prefWidth="300.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel_OnAction" text="Cancel" />
                  <Button fx:id="create" disable="true" layoutX="238.0" mnemonicParsing="false" onAction="#create_OnAction" text="Create" />
                  <Label fx:id="lblTitle" layoutX="115.0" layoutY="5.0" text="Create File" />
               </children>
            </Pane>
         </top>
         <center>
            <Pane prefHeight="42.0" prefWidth="300.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="lblName" layoutX="65.0" layoutY="14.0" text="Name" />
                  <TextField fx:id="txt" layoutX="65.0" layoutY="42.0" onKeyPressed="#txt_OnKeyPressed" onKeyTyped="#txtOnKeyTyped" />
               </children>
            </Pane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
